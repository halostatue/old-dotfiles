#autoload -U
function compadd()
{
  local -A args
  zparseopts -E -A args J:
  if [[ $args[-J] == all-expansions ]]; then
    builtin compadd -A kept "$@"
    kept=( ${(Q)${(z)kept}} )
  fi
  builtin compadd "$@"
}
local result
_main_complete _expand
result=$?
unfunction compadd
return result

# This will expand the kept list into the current ZLE buffer.
# Part of Bart Schaefer's Keeper functions.
