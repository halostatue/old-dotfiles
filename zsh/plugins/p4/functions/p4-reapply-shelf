#autoload -U

local -i shelf_id=${1}
local -a items

awkprog=$(mktemp)

cat > ${awkprog} <<EOS
/can't unshelve/  {
                    FS=" - can't unshelve";
                    print \$1;
                  }
EOS

p4 unshelve -ns ${1} | \
  awk -f ${awkprog} | \
  p4 -x - revert

p4 unshelve -s ${1}
